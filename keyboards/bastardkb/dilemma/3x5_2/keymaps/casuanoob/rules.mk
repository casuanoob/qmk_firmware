# Loads /users/casuanoob userspace.
USER_NAME := casuanoob
KEYMAP_ENABLE = split34  # Use "split34" keymap (from /users/casuanoob).

# Keymap supports OLED screen. Not enabled by default.
OLED_DRIVER = custom
OLED_SUPPORTED = yes
